 // Define
#define IDD_AWE             101
#define IDI_AWE             104
#define IDC_WINDOW0TEXT     1006
#define IDC_WINDOW0STORAGE  1007
#define IDC_WINDOW1STORAGE  1008
#define IDC_WINDOW1TEXT     1009
