  LTEXT     "Window 1:", IDC_STATIC, 4, 28, 35, 8
  COMBOBOX  IDC_WINDOW1STORAGE, 44, 25, 80, 58, CBS_DROPDOWNLIST | WS_TABSTOP
  EDITTEXT  IDC_WINDOW1TEXT, 132, 25, 152, 14, ES_AUTOHSCROLL
END

 // Icon
IDI_AWE ICON DISCARDABLE "AWE.ico"
