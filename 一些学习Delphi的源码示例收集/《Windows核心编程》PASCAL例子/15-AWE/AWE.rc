BEGIN
  LTEXT     "Window 0:", IDC_STATIC, 4, 6, 35, 8
  COMBOBOX  IDC_WINDOW0STORAGE, 44, 4, 80, 58, CBS_DROPDOWNLIST | WS_TABSTOP
